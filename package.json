{
  "name": "algos-ds",
  "version": "1.0.0",
  "description": "algos and ds practice",
  "main": "index.js",
  "repository": "git@github.com:patrickocoffeyo/algos-ds.git",
  "author": "Patrick Coffey <patrickcoffey48@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc --build tsconfig.dist.json",
    "watch": "yarn build -w",
    "format": "prettier --write './src/**/*.{js,ts}' && yarn lint --fix",
    "husky:commit-msg": "commitlint --edit $1",
    "husky:pre-commit": "lint-staged",
    "husky:pre-push": "lint-staged && yarn test",
    "lint": "eslint --ext .js,.ts .",
    "test": "jest --coverage --silent",
    "type": "tsc --noEmit",
    "twatch": "jest --no-coverage --watch --verbose",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@semantic-release/github": "^7.2.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
